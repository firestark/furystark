{
    "name": "firestark/project",
    "type": "project",
    "description": "PHP7+ NON-MVC framework focused on the business logic.",
    "keywords": [
        "framework",
        "non-mvc",
        "business logic",
        "firestark"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aron Wouters",
