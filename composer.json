            "homepage": "https://eyedouble.nl/"
        }
    ],
    "require": {
        "illuminate/container": "@stable",
        "firebase/php-jwt": "@stable",
        "php": "^7.1.3",
        "relay/relay": "2.x-dev",
        "laminas/laminas-diactoros": "^2.1@dev",
        "laminas/laminas-httphandlerrunner": "^1.1@dev",
        "league/route": "dev-master",
        "jenssegers/blade": "dev-master"
    },
    "require-dev": {
        "middlewares/debugbar": "dev-master",
        "middlewares/whoops": "dev-master",
        "mockery/mockery": "^1.0@dev"
